{
  "name": "concept-map",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "server:dev:dev": "dotenv -e .env.development -- turbo run server:dev",
    "server:build:dev": "dotenv -e .env.development -- turbo run server:build",
    "server:build:production": "dotenv -e .env.production -- turbo run server:build",
    "server:start:production": "dotenv -e .env.production -- turbo run server:start",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "express": "^4.21.2"
  }
}
