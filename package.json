{
  "name": "concept-map",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "server:dev:dev": "dotenv -e .env.development -- turbo run server:dev",
    "server:build:dev": "turbo run server:build",
    "server:start:dev": "npx dotenv -e .env.development -- npx turbo run server:start",
    "server:build:production": "dotenv -e .env.production -- turbo run server:build",
    "server:start:production": "dotenv -e .env.production -- turbo run server:start",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "creator": "^0.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "react-awesome-reveal": "^4.3.1",
    "react-icons": "^5.4.0",
    "react-reveal": "^1.2.2",
    "swiper": "^11.2.1"
  }
}
